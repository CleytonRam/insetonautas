; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Portraits & Emotes Dialogue Example"
config/tags=PackedStringArray("example")
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

State="*res://state.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[dialogue_manager]

runtime/state_autoload_shortcuts=["State"]
runtime/balloon_path="res://dialogue/dialogue_balloon.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="DialogueExample"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[internationalization]

locale/translations_pot_files=PackedStringArray("res://scenes/main.dialogue")

[layer_names]

2d_physics/layer_5="Actionables"
